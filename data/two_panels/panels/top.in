[toplevel-settings]
monitor=0
background-type='gnome'
edge='top'
height=26
auto-hide=false
alignment='center'
background-file='@prefix@/share/simple-panel/images/background.png'
enable-font-size=false

[0]
plugin-type='space'
has-schema=true
is-expanded=false
position=0
size=2

[1]
plugin-type='menumodel'
has-schema=true
is-expanded=false
position=1
icon-name='start-here-symbolic'
is-internal-menu=true
is-system-menu=true
is-menu-bar=true

[2]
plugin-type='launchtaskbar'
has-schema=true
can-expand=true
launch-buttons=['menu://applications/System/pcmanfm.desktop', 'menu://applications/System/gnome-terminal.desktop', 'menu://applications/Internet/firefox.desktop']
position=2
mode='launchbar'

[3]
plugin-type='space'
has-schema=true
can-expand=true
is-expanded=true
size=2
position=3

[4]
plugin-type='netstat'
has-schema=true
is-expanded=false
position=4

[5]
plugin-type='volumealsa'
has-schema=true
is-expanded=false
position=5
card-id='default'
channel-id='Master'
mixer='gnome-alsamixer'
