<schemalist gettext-domain="@GETTEXT_PACKAGE@">
  <schema id="org.simple.panel.toplevel.wincmd">
    <key name="left-button-command" enum="org.simple.panel.WcCommand">
      <default>'iconify'</default>
    </key>
    <key name="middle-button-command" enum="org.simple.panel.WcCommand">
      <default>'shade'</default>
    </key>
    <key name="toggle-iconify-and-shade" type="b">
      <default>false</default>
    </key>
    <key name="image" type="s">
      <default>'window-manager'</default>
    </key>
  </schema>
  <schema id="org.simple.panel.toplevel.deskno">
    <key name="bold-font" type="b">
      <default>false</default>
    </key>
    <key name="wm-labels" type="b">
      <default>true</default>
    </key>
  </schema>
  <schema id="org.simple.panel.toplevel.kbled">
    <key name="numlock-on" type="b">
      <default>true</default>
    </key>
    <key name="capslock-on" type="b">
      <default>true</default>
    </key>
    <key name="scrllock-on" type="b">
      <default>true</default>
    </key>
  </schema>
  <schema id="org.simple.panel.toplevel.xkb">
    <key name="layout-per-window" type="b">
      <default>false</default>
    </key>
    <key name="keep-system-layouts" type="b">
      <default>true</default>
    </key>
    <key name="use-existing-options" type="b">
      <default>true</default>
    </key>
    <key name="image-size" type="i">
      <default>2</default>
    </key>
    <key name="display-type" enum="org.simple.panel.DispType">
      <default>'text'</default>
    </key>
    <key name="keyboard-model" type="s">
      <default>'pc105'</default>
    </key>
    <key name="keyboard-layouts" type="s">
      <default>'us,ru'</default>
    </key>
    <key name="keyboard-layout-variants" type="s">
      <default>','</default>
    </key>
    <key name="layout-changing-options" type="s">
      <default>'grp:shift_caps_toggle'</default>
    </key>
    <key name="advanced-options" type="s">
      <default>''</default>
    </key>
  </schema>
  <schema id="org.simple.panel.toplevel.launchtaskbar">
    <key name="mode" enum="org.simple.panel.LtbMode">
      <default>'launchtaskbar'</default>
    </key>
    <key name="launch-buttons" type="as">
      <default>[]</default>
    </key>
    <key name="tooltips" type="b">
      <default>true</default>
    </key>
    <key name="icons-only" type="b">
      <default>false</default>
    </key>
    <key name="all-desktops" type="b">
      <default>true</default>
    </key>
    <key name="same-monitor-only" type="b">
      <default>false</default>
    </key>
    <key name="use-wheel" type="b">
      <default>true</default>
    </key>
    <key name="urgency-hint" type="b">
      <default>true</default>
    </key>
    <key name="flat-buttons" type="b">
      <default>false</default>
    </key>
    <key name="grouped-tasks" type="b">
      <default>false</default>
    </key>
    <key name="task-width" type="i">
      <default>200</default>
    </key>
    <key name="spacing" type="i">
      <default>1</default>
    </key>
  </schema>
</schemalist>
