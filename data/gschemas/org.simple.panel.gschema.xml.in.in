<schemalist gettext-domain="@GETTEXT_PACKAGE@">
  <schema id="org.simple.panel" path="/org/simple/panel/">
    <key name="is-dark" type="b">
      <default>false</default>
      <_summary>Use dark theme for panel.</_summary>
      <_description>If true, dark theme variant will be used.</_description>
    </key>
    <key name="is-custom" type="b">
      <default>false</default>
      <_summary>Use custom CSS file for theming a panel</_summary>
      <_description>If true, custom CSS file will be loaded from css-file key.</_description>
    </key>
    <key name="css" type="s">
      <default>""</default>
      <_summary>Panel CSS theme</_summary>
      <_description>Specifies the file to be used as the panel theme.</_description>
    </key>
    <key name="logout-command" type="s">
      <default>"gnome-logout"</default>
      <_summary>Command for logging out.</_summary>
      <_description>Command for logging out from the session</_description>
    </key>
    <key name="shutdown-command" type="s">
      <default>"gnome-logout"</default>
      <_summary>Command for shutting down.</_summary>
      <_description>Command for shutting down the computer</_description>
    </key>
    <key name="terminal-command" type="s">
      <default>"sakura"</default>
      <_summary>Terminal emulator</_summary>
      <_description>Command for using as terminal emulator.</_description>
    </key>
    <key name="confirm-panel-remove" type="b">
      <default>true</default>
      <_summary>Confirm panel removal</_summary>
      <_description>If true, a dialog is shown asking for confirmation if the user wants to remove a panel.</_description>
    </key>
  </schema>
</schemalist>
