[6]
plugin-type='batt'
has-schema=true
is-expanded=false
position=6
show-percentage=true

[7]
plugin-type='tray'
has-schema=false
is-expanded=false
position=7

[8]
plugin-type='dclock'
has-schema=true
is-expanded=false
