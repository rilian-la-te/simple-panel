<schemalist gettext-domain="@GETTEXT_PACKAGE@">
  <schema id="org.simple.panel.toplevel.dclock">
    <key name="clock-format" type="s">
      <default>'%R'</default>
    </key>
    <key name="tooltip-format" type="s">
      <default>'%A %x'</default>
    </key>
    <key name="click-action" type="ms">
      <default>nothing</default>
    </key>
    <key name="icon-only" type="b">
      <default>false</default>
    </key>
    <key name="bold-font" type="b">
      <default>true</default>
    </key>
    <key name="center-text" type="b">
      <default>true</default>
    </key>
  </schema>
  <schema id="org.simple.panel.toplevel.dirmenu">
    <key name="name" type="ms">
      <default>nothing</default>
    </key>
    <key name="path" type="s">
      <default>'~'</default>
    </key>
    <key name="icon" type="s">
      <default>'system-file-manager'</default>
    </key>
  </schema>
  <schema id="org.simple.panel.toplevel.space">
    <key name="size" type="i">
      <default>2</default>
    </key>
  </schema>
  <schema id="org.simple.panel.toplevel.menu">
    <key name="name" type="ms">
      <default>nothing</default>
    </key>
    <key name="icon" type="s">
      <default>'start-here-symbolic'</default>
    </key>
    <key name="highlight-color" type="s">
      <default>'rgba(50,50,50,0.37)'</default>
    </key>
    <key name="layout" type="a(ssss)">
      <default>[("system","","",""),("separator","","",""),("item","run","application-x-executable","command"),("separator","","",""),("item","logout","gnome-logout","command"),("item","shutdown","gnome-shutdown","command")]</default>
    </key>
  </schema>
  <schema id="org.simple.panel.toplevel.menumodel">
    <key name="menu-name" type="ms">
      <default>nothing</default>
    </key>
    <key name="icon-name" type="ms">
      <default>'start-here-symbolic'</default>
    </key>
    <key name="is-system-menu" type="b">
      <default>true</default>
    </key>
    <key name="is-internal-menu" type="b">
      <default>true</default>
    </key>
    <key name="is-menu-bar" type="b">
      <default>true</default>
    </key>
    <key name="model-file" type="ms">
      <default>nothing</default>
    </key>
    <key name="model-name" type="s">
      <default>'appmenu'</default>
    </key>
  </schema>
</schemalist>
